<h1>Trainers</h1>

<table style="width:100%">
  <tr>
    <th>Brand</th>
    <th>Name</th>
    <th>Description</th>
    <th>Quantity</th>
    <th>Buy Cost</th>
    <th>Sell Cost</th>
  </tr>
  <% for trainer in @trainers %>
  <tr>
    <td><%= trainer.brand.name%></td>
    <td><%= trainer.trainer_name %></td>
    <td><%= trainer.description %></td>
    <td><%= trainer.quantity %></td>
    <td><%= trainer.buy_cost %></td>
    <td><%= trainer.sell_price %></td>
  </tr>
  <% end %>

</table>
